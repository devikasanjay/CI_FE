useEffect(() => {
  const merged = [
    ...selectedCWWorkspaces,
    ...selectedAribaWorkspaces,
  ];

  // Remove duplicates by id
  const unique = Array.from(
    new Map(merged.map((w) => [w.id, w])).values()
  );

  setSelectedWorkspace(unique);
}, [selectedCWWorkspaces, selectedAribaWorkspaces]);








